{
  "name": "poetainos",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --ssr",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write resources/js/",
    "docker-build": "cd laradock && docker compose build nginx mariadb php-fpm workspace",
    "docker-up": "cd laradock && docker compose up -d nginx mariadb workspace && docker compose exec --user=laradock workspace bash",
    "docker-down": "cd laradock && docker compose stop",
    "docker-restart": "npm run workspace-stop && npm run workspace-start",
    "translate": "php artisan translatable:export es",
    "translate-check": "php artisan translatable:inspect-translations es"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@inertiajs/vue3": "^1.2.0",
    "@khmyznikov/pwa-install": "^0.3.9",
    "@prettier/plugin-php": "^0.22.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/server-renderer": "^3.4.38",
    "@vueuse/core": "^10.11.1",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "crop-url": "^4.0.1",
    "date-fns": "^3.6.0",
    "eslint": "^9.9.1",
    "eslint-plugin-vue": "^9.27.0",
    "laravel-echo": "github:laravel/echo",
    "laravel-vite-plugin": "^1.0.5",
    "linkify-html": "^4.1.3",
    "linkify-plugin-mention": "^4.1.3",
    "linkifyjs": "^4.1.3",
    "lodash-es": "^4.17.21",
    "markdown-it": "^14.1.0",
    "masonry-simple": "^3.2.1",
    "millify": "^6.1.0",
    "prettier": "^3.3.3",
    "pusher-js": "^8.4.0-rc2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.2",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-pwa": "^0.20.2",
    "vite-plugin-vuetify": "^2.0.4",
    "vue": "^3.4.38",
    "vue-i18n": "^9.14.0",
    "vuetify": "^3.7.1",
    "workbox-build": "^7.1.1"
  }
}
