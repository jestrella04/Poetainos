{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1.2",
    "beyondcode/laravel-websockets": "^1.14",
    "doctrine/dbal": "^3.0",
    "guzzlehttp/guzzle": "^7.2",
    "inertiajs/inertia-laravel": "^0.6.8",
    "laravel-notification-channels/facebook-poster": "^5.1",
    "laravel-notification-channels/twitter": "^8.0",
    "laravel-notification-channels/webpush": "^7.0",
    "laravel/framework": "^10.0",
    "laravel/passport": "^11.0",
    "laravel/sanctum": "^3.2",
    "laravel/socialite": "^5.0",
    "laravel/tinker": "^2.7",
    "mews/captcha": "^3.3",
    "pusher/pusher-php-server": "^7.0",
    "spatie/image-optimizer": "^1.7",
    "spatie/laravel-ignition": "^2.0",
    "spatie/laravel-sitemap": "^6.0",
    "staudenmeir/laravel-adjacency-list": "^1.5",
    "tightenco/ziggy": "^1.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "kkomelin/laravel-translatable-string-exporter": "^1.10",
    "laravel/breeze": "^1.27",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0",
    "spatie/laravel-ignition": "^2.3"
  },
  "autoload": {
    "files": [
      "app/Helpers/Helper.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
